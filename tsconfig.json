{
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react",
    "lib": ["es5", "es6", "dom"],
    "module": "commonjs",
    "noImplicitAny": false,
    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5",
    "typeRoots": ["custom_typings"]
  },
  "include": ["./src/**/*.tsx", "./src/**/*.ts", "src/custom.d.ts"],
  "exclude": ["dist", "build", "node_modules"]
}
